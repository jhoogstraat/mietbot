{
  "name": "mietbot",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-specifier-resolution=node out/test.js",
    "build": "npx tsc",
    "start-redis": "docker run -d --rm --name redis -p 6379:6379 redis",
    "start-mongodb": "docker run -d --rm --name mongo -p 27017:27017 -e MONGO_INITDB_ROOT_USERNAME=mietbot -e MONGO_INITDB_ROOT_PASSWORD=mietbot mongo",
    "deploy-dc-commands": "npx tsc && node out/discord-connector/commands.js",
    "start": "node --experimental-specifier-resolution=node out/main.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bullmq": "^1.76.4",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "mongodb": "^4.4.1",
    "node-fetch": "^3.2.3",
    "node-html-parser": "^5.3.3",
    "node-schedule": "^2.1.0",
    "puppeteer": "^13.5.1"
  },
  "devDependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "@types/node": "^17.0.21",
    "@types/node-schedule": "^1.3.2",
    "discord-api-types": "^0.32.0",
    "eslint": "^8.10.0",
    "typescript": "^4.6.2"
  }
}